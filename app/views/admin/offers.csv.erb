<%- headers = ['Subject', 'Num', 'Section', 'Title', 'Type', 'Status', 'Student Name', 'Date Updated'] -%>
<%= CSV.generate_line headers %>
<%- @offers.each do |obj| -%>
<% if obj.student.nil? %>
<%= CSV.generate_line([obj.course.subject, obj.course.num, obj.course.section, obj.course.title, obj.app_type, obj.status,
    "N/A", "N/A", "N/A", "N/A", obj.updated_at]).html_safe -%>
<% else %>
<%= CSV.generate_line([obj.course.subject, obj.course.num, obj.course.section, obj.course.title, obj.app_type, obj.status,
    obj.student.fullname, obj.student_accepted, obj.advisor_accepted, obj.instructor_accepted, obj.updated_at]).html_safe -%>
<% end %>

<%- end -%>