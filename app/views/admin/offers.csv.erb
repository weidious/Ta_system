<%- headers = ['Course', 'Type', 'Status', 'Student Name', 'Student Accepted', 'Advisor Accepted', 'Instructor Accepted', 'Date Updated'] -%>
<%= CSV.generate_line headers %>
<%- @offers.each do |obj| -%>
<% if obj.student.nil? %>
<%= CSV.generate_line([obj.course.caption, obj.app_type, obj.status,"N/A", "N/A", "N/A", "N/A", obj.updated_at]).html_safe -%>
<% else %>
<%= CSV.generate_line([obj.course.caption, obj.app_type, obj.status,
    obj.student.fullname, obj.student_accepted, obj.advisor_accepted, obj.instructor_accepted, obj.updated_at]).html_safe -%>
<% end %>

<%- end -%>